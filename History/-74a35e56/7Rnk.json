  },
  {
    "key": "ctrl+n",
    "command": "workbench.files.action.createFileFromExplorer"
  },
  {
    "key": "ctrl+shift+alt",
    "command": "extension.advancedNewFile"
  },
  {
    "key": "ctrl+alt+n",
    "command": "-extension.advancedNewFile"
  },
  {
    "key": "alt+a",
