  },
  "explorer.confirmDragAndDrop": false,
  "interactiveSession.editor.wordWrap": "on",
  "screencastMode.fontSize": 20,
  "screencastMode.verticalOffset": 0,
  "diffEditor.wordWrap": "on",
  "editor.wordWrap": "on",

  //By me
