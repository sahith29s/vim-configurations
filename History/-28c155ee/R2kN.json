  "git.openRepositoryInParentFolders": "never",
  "editor.lineNumbers": "relative",
  "editor.cursorBlinking": "expand",
  "[handlebars]": {
    "editor.defaultFormatter": "vscode.html-language-features"
  },
  // vim normal mode
  "vim.normalModeKeyBindings": [
    // for surrounding a word with any (quotes, prentheis, flower brackets)
    { "before" : ["<leader>" , "s"], "after" : ["y", "s" , "i" , "w"] },

    { "before" : ["<leader>" , "s" ], "after" : ["S"] },

    { "before" : ["<leader>" , "v" ], "after" : ["V"] },

    { "before" : ["<leader>" , "a" ], "after" : ["A"] },

    { "before" : ["<leader>" , "f" ], "after" : ["I" ] },


    { "before": ["<leader>", "d"], "after": ["d", "d"] },

    { "before": ["<leader>", "i"], "after": ["c", "i", "t"] },

    // tab
    { "before": ["<tab>"], "commands": ["workbench.action.nextEditor"] },

    { "before": ["<n-tab>"], "commands": ["workbench.action.previousEditor"] }

  ],

  // Vim insert mode
  "vim.insertModeKeyBindings": [
    {
      "before" : ["j" , "s"],
    "after" : ["j" , "k" , "S"]
    },

    { "before" : ["j" , "a"], "after" : ["j" , "k" , "A"] },

    { "before" : ["j" , "i"], "after" : ["j" , "k" , "I"] },

    { "before" : ["j" , "v"], "after" : ["j" , "k" , "V"]},

    {"before" : ["j" , "d"], "after" : ["j" , "k" , "o"] },

    { "before": ["j", "j"], "after": ["<Esc>"] },

    { "before": ["j", "f"], "after": ["<Esc>"] },

    { "before": ["j", "k"], "after": ["<Esc>"] },

    // loda behen chod kam hi nahi krta
  ],

  "vim.visualModeKeyBindings": [
    { "before": [">"], "after": [">", "g", "v"] },
    { "refore": ["<"], "after": ["<", "g", "v"] }
  ],

  // Vim normal mode
  "vim.normalModeKeyBindingsNonRecursive": [

    {"before" : ["<leader>" , "x", "r"] , "commands"  : [":vsplit"]},
    {"before" : ["<leader>" , "x", "d"] , "commands"  : [":split"]},
    // {"before" : ["<leader>" , "x", "l"] , "commands"  : ["workbench.action.focusLeftGroup"]},
    {"before" : ["<leader>" , "x", "r"] , "commands"  : ["workbench.action.focusRightGroup"]},
    {"before" : ["<leader>" , "x", "u"] , "commands"  : ["workbench.action.focusBelowGroup"]},
    // {"before" : ["<leader>" , "x", "u"] , "commands"  : ["workbench.action.focusAboveGroup"]},

    {"before" : ["<leader>" , "u"] , "commands" : [""]},

    // Tabs
    { "before": ["H"], "commands": [":bprevious"] },

    { "before": ["L"], "commands": [":bnext"] },

    { "before": ["<S-k>"], "commands": [ { "command": "editor.action.showDefinitionPreviewHover" } ] },




    { "before": ["<space>" , "m"], "commands": ["whichkey.show"] },

    { "before": ["<leader>", "c"], "after": ["c", "a", "w"] },

    { "before": ["<leader>", "r"], "after": ["d", "a", "w"] },


    { "before": ["<leader>", "e"], "commands": ["workbench.action.toggleSidebarVisibility"] },

    { "before": ["<leader>", "j"], "after": ["_"] },

    { "before": ["<leader>", "k"], "after": ["$"] }
  ],

  //vim leader
  "vim.leader": "<space>",
  "vim.handleKeys": {
    "<C-a>": false,
    "<C-f>": false,
    // VS Code new marker @ next occurence
    "<C-c>": false,
    // Cut
    "<C-x>": false,
    // Paste
    "<C-v>": false,
    "<C-z>": false,
    "<C-y>": false
  },
  "sync.gist": "0347f564969e6ea0530a7658bdcf7395",
  "editor.quickSuggestions": {
    "other": "on",
    "comments": "off",
    "strings": "off"
  },
  "editor.formatOnSave": true,
  "[django-html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "zenMode.hideLineNumbers": false,
  "zenMode.hideStatusBar": false,
  "zenMode.centerLayout": false,
  "zenMode.hideTabs": false,
  "workbench.iconTheme": "material-icon-theme",
  "workbench.editor.showTabs": true,
  "editor.minimap.autohide": true,
  "editor.minimap.renderCharacters": false,
  "[typescriptreact]": {
    "editor.defaultFormatter": "vscode.typescript-language-features"
  },
  "javascript.updateImportsOnFileMove.enabled": "always",
  "material-ui-snippets.showNotesOnStartup": false,
  "workbench.colorCustomizations": {
    "editorCursor.foreground": "#FC6600",
    "terminalCursor.foreground": "#ffff00"
},

"typescript.updateImportsOnFileMove.enabled": "always",
"workbench.sideBar.location": "right",
"jupyter.askForKernelRestart": false,
"workbench.colorTheme": "Monokai Hacker",
"window.title": "Sahith !!! ",
"window.zoomLevel": -1
}
