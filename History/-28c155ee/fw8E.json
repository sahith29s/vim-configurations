    "jsonc": true,


    "latex": true,


    "less": true,


    "lua": true,


    "makefile": true,


    "markdown": true,


    "objective-c": true,


    "objective-cpp": true,


    "perl6": true,


    "php": true,


    "powershell": true,


    "jade": true,


    "python": true,


    "r": true,


    "razor": true,


    "ruby": true,


    "rust": true,


